{
  "expo": {
    "name": "Daily pet",
    "description": "Get your daily pet!",
    "slug": "daily-native",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#cae4db"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.vladimirgusev.dailypet"
    },
    "android": {
      "package": "com.vladimirgusev.dailypet"
    }
  }
}